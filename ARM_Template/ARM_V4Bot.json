{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appService_willssamplebot_name": {
      "type": "string"
    },
    "appServicePlan_willssamplebot_location": {
      "type": "string"
    },
    "appServicePlan_willssamplebot_name": {
      "type": "string"
    },
    "appServicePlan_willssamplebot_resourceGroup": {
      "type": "string"
    },
    "willssamplebotStorageAccount": {
      "type": "String"
    },
    "storageaccount_willssamplebot": {
      "type": "string"
    },
    "appServicePlan_willssamplebot_skuName": {
      "type": "string"
    },
    "appServicePlan_willssamplebot_skuTier": {
      "type": "string"
    },
    "appServicePlan_willssamplebot_skuFamily": {
      "type": "string"
    },
    "appServicePlan_willssamplebot_skuSize": {
      "type": "string"
    },
    "appServicePlan_willssamplebot_workerSizeId": {
      "type": "int"
    },
    "willssamplebotStorageAccount_location": {
      "type": "String"
    },
    "willssamplebotStorageAccountType": {
      "type": "String"
    }
  },
  "resources": [
    {
      "location": "[parameters('appServicePlan_willssamplebot_location')]",
      "name": "[parameters('appService_willssamplebot_name')]",
      "type": "Microsoft.Web/sites",
      "apiVersion": "2015-08-01",
      "tags": {
        "[concat('hidden-related:', resourceId(parameters('appServicePlan_willssamplebot_resourceGroup'),'Microsoft.Web/serverfarms', parameters('appServicePlan_willssamplebot_name')))]": "empty"
      },
      "dependsOn": [
        "[resourceId('Microsoft.Web/serverFarms', parameters('appServicePlan_willssamplebot_name'))]",
        "[resourceId('Microsoft.Storage/storageAccounts', parameters('willssamplebotStorageAccount'))]"
      ],
      "kind": "functionapp",
      "properties": {
        "name": "[parameters('appService_willssamplebot_name')]",
        "serverFarmId": "[resourceId(parameters('appServicePlan_willssamplebot_resourceGroup'),'Microsoft.Web/serverfarms', parameters('appServicePlan_willssamplebot_name'))]",
        "kind": "functionapp",
        "httpsOnly": true
      },
      "resources": [
        {
          "name": "appsettings",
          "type": "config",
          "apiVersion": "2015-08-01",
          "dependsOn": [
            "[concat('Microsoft.Web/Sites/', parameters('appService_willssamplebot_name'))]"
          ],
          "properties": {
            "WEBSITE_CONTENTAZUREFILECONNECTIONSTRING": "[concat('DefaultEndpointsProtocol=https;AccountName=', parameters('storageaccount_willssamplebot'), ';AccountKey=', listKeys(resourceId('Microsoft.Storage/storageAccounts', parameters('storageaccount_willssamplebot')), '2015-06-15').key1)]",
            "WEBSITE_CONTENTSHARE": "[toLower(parameters('appService_willssamplebot_name'))]",
            "AzureWebJobsDashboard": "[concat('DefaultEndpointsProtocol=https;AccountName=', parameters('storageaccount_willssamplebot'), ';AccountKey=', listKeys(resourceId('Microsoft.Storage/storageAccounts', parameters('storageaccount_willssamplebot')), '2015-06-15').key1)]",
            "AzureWebJobsStorage": "[concat('DefaultEndpointsProtocol=https;AccountName=', parameters('storageaccount_willssamplebot'), ';AccountKey=', listKeys(resourceId('Microsoft.Storage/storageAccounts', parameters('storageaccount_willssamplebot')), '2015-06-15').key1)]",
            "FUNCTIONS_EXTENSION_VERSION": "~2"
          }
        }
      ]
    },
    {
      "location": "[parameters('appServicePlan_willssamplebot_location')]",
      "name": "[parameters('appServicePlan_willssamplebot_name')]",
      "type": "Microsoft.Web/serverfarms",
      "apiVersion": "2015-08-01",
      "sku": {
        "name": "[parameters('appServicePlan_willssamplebot_skuName')]",
        "tier": "[parameters('appServicePlan_willssamplebot_skuTier')]",
        "family": "[parameters('appServicePlan_willssamplebot_skuFamily')]",
        "size": "[parameters('appServicePlan_willssamplebot_skuSize')]"
      },
      "properties": {
        "name": "[parameters('appServicePlan_willssamplebot_name')]"
      }
    },
    {
      "location": "[parameters('willssamplebotStorageAccount_location')]",
      "name": "[parameters('willssamplebotStorageAccount')]",
      "type": "Microsoft.Storage/storageAccounts",
      "apiVersion": "2015-06-15",
      "tags": {
        "[concat('hidden-related:', concat('/providers/Microsoft.Web/sites/', parameters('appService_willssamplebot_name')))]": "empty"
      },
      "properties": {
        "accountType": "[parameters('willssamplebotStorageAccountType')]"
      }
    }
  ]
}